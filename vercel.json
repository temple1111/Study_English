{
  "version": 2,
  "builds": [
    {
      "src": "eng_learner_web_app/backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "eng_learner_web_app/backend/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "eng_learner_web_app/frontend/build/index.html"
    }
  ]
}