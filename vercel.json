{
  "version": 2,
  "builds": [
    {
      "src": "eng_learner_web_app/frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" },
      "root": "eng_learner_web_app/frontend"
    },
    {
      "src": "eng_learner_web_app/backend/main.py",
      "use": "@vercel/python",
      "root": "eng_learner_web_app/backend"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "eng_learner_web_app/backend/main.py"
    }
  ]
}